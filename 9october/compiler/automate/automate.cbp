				<Option compiler="new_gnu_gcc_compiler" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/automate" prefix_auto="1" extension_auto="1" />
